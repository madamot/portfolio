{
  "name": "page-deploy-site",
  "version": "1.0.0",
  "description": "Build pages from DatoCMS",
  "main": "app.js",
  "scripts": {
    "test:watch": "yarn run test --watch",
    "test": "jest --coverage",
    "build": "yarn tsc && yarn run copy-packages && yarn run copy-files",
    "copy-packages": "cp package.json dist/package.json",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "serve": "http-server -c-1 dist/",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.425.0",
    "@aws-sdk/client-secrets-manager": "^3.431.0",
    "axios": "^1.5.1",
    "esbuild": "^0.19.4",
    "graphql": "^16.8.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^29.4.0",
    "@types/mustache": "^4.2.3",
    "copyfiles": "^2.4.1",
    "http-server": "^14.1.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/introspection": "4.0.0"
  }
}
